{
    "manifest_version": 2,
    "name": "ix.io-syntax",
    "short_name": "cli paste syntax highlighter",
    "description": "ix.io & sprunge.us & clbin.com - syntax highlighter",
    "version": "1.2.4",
    "icons": { 
        "16": "icon16.png",
        "48": "icon48.png",
        "128": "icon128.png" 
    },
    "permissions": [
        "tabs",
        "<all_urls>",
        "storage",
        "http://*/*",
        "https://*/*"
    ],
    "content_scripts": [ 
        { 
            "all_frames": false, 
            "js": [ "ui/js/extension.js" ],
            "matches": [ 
                "*://ix.io/*" ,
                "*://sprunge.us/*",
                "*://clbin.com/*"
            ],
            "run_at": "document_start"
        }
    ],
    "web_accessible_resources": [
        "ui/js/extension.js",
        "ui/js/libs.min.js",
        "ui/css/ixio.min.css",
        "ui/img/brown_papersq.png",
        "ui/img/school_book.png",
        "ui/img/pojoaque.jpg"
    ]
}